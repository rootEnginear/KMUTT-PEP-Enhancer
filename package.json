{
  "name": "kmutt-pep-enhancer",
  "version": "1.0.0",
  "description": "KMUTT Past Exam Paper Enhancer for better paper browsing experience.",
  "main": "",
  "scripts": {
    "build-css": "postcss --use autoprefixer --use cssnano -o src/style.min.css src/style.css",
    "build-js": "terser src/script.js -o src/script.min.js",
    "build": "npm run build-css && npm run build-js",
    "es-check": "es-check es6 src/script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rootEnginear/KMUTT-PEP-Enhancer.git"
  },
  "keywords": [
    "KMUTT",
    "Exam"
  ],
  "author": "Suthep Chanchuphol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rootEnginear/KMUTT-PEP-Enhancer/issues"
  },
  "homepage": "https://github.com/rootEnginear/KMUTT-PEP-Enhancer#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.12",
    "es-check": "^6.1.1",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "terser": "^5.14.2"
  }
}
